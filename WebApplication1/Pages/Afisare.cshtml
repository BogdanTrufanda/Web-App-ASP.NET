@page
@model WebApplication1.Pages.AfisareModel
@{
    ViewData["Title"] = "Search";
}

    <center><h1>Search page</h1></center>
<br />

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].justID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].Path)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].Size)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].cDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].mDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].Resolution)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PGO[0].Extension)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model.PGO)
        {
            <tr style="font-size:small">
                <td>
                    @Html.DisplayFor(modelItem => item.justID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Path)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Size)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Extension)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Resolution)
                </td>
            </tr>
        }
    </tbody>
</table>


<br />
<p>
    @Html.DisplayFor(model => model.PGO3[0].Contor)
</p>
<br />
@foreach (var item in Model.PGO2)
{
<div class="row">
    @Html.DisplayFor(modelItem => item.Pozaname)
    <br />
    <img src=@item.Pozaname style="height:200px;width:200px;" />
</div>
}

<br />
<br />
